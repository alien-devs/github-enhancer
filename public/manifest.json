{
	"name": "Hello Extensions",
	"description": "Base Level Extension",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Open the popup"
	},
	"icons": {
		"16": "logo192.png",
		"48": "logo192.png",
		"128": "logo192.png"
	},
	"optional_permissions": ["<all_urls>"],
	"content_scripts": [
		{
			"css": ["index.css"],
			"matches": ["https://github.com/*"]
		}
	]
}
